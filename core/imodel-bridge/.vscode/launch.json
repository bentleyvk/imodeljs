    "version": "0.2.0",
    "configurations": [
        {
            "presentation": {
              "group": "6_Misc"
            },
            "name": "2pc: MockReader.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/2pc/test/assets/ToyTileReader.py",
            "console": "integratedTerminal",
            "args": [
              "[::]:50051",
              "${workspaceFolder}/src/2pc/test/assets/toytile.json"
            ],
            "env": {
              "PYTHONPATH": "${workspaceFolder}/lib/2pc/generated"
            }
          },
          {
            "name": "2pc",
            "presentation": {
              "group": "6_Misc"
            },
            "cwd": "${workspaceFolder}",
            "type": "pwa-node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "args": [
              "--no-timeouts",
              "lib/2pc/test/**/*.test.js"
            ],
            "skipFiles": [
              "<node_internals>/**/*.js",
              "node_modules/rxjs/**/*"
            ],
            "outFiles": [
              "${workspaceFolder}/lib/2pc/**/*.js"
            ],
            "env": {
              "toytile_inline_typescript_reader": "1",
              "//toytile_server_address": "[::]:50051"
            }
          }
    ]
}