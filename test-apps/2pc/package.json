{
  "name": "2pc",
  "private": true,
  "license": "MIT",
  "description": "Example of two-process iModel connector",
  "scripts": {
    "compile": "npm run build",
    "build": "npm run proto-js && npm run proto-ts && npm run build:tsc && npm run lint",
    "build:tsc": "tsc 1>&2",
    "proto-js": "grpc_tools_node_protoc --grpc_out=grpc_js:src/generated --js_out=\"import_style=commonjs,binary:src/generated\" -I ./protobuf protobuf/*.proto",
    "proto-ts": "grpc_tools_node_protoc --plugin=protoc-gen-ts=D:/github/imodeljs/test-apps/2pc/node_modules/.bin/protoc-gen-ts.cmd --ts_out=grpc_js:src/generated -I ./protobuf protobuf/*.proto",
    "lint": "npm run lint:eslint",
    "lint:fix": "eslint --ignore-pattern \"src/**/*.d.ts\" --fix ./src/**/*.{ts,tsx} 1>&2",
    "lint:eslint": "eslint --ignore-pattern \"src/**/*.d.ts\" ./src/**/*.{ts,tsx} 1>&2",
    "clean": "rimraf lib .rush/temp/package-deps*.json",
    "test": "",
    "docs": "",
    "cover": ""
  },
  "repository": {},
  "dependencies": {
    "@grpc/grpc-js": "^1.3.4",
    "google-protobuf": "^3.13.0"
  },
  "devDependencies": {
    "@bentley/build-tools": "workspace:*",
    "@bentley/eslint-plugin": "workspace:*",
    "@types/node": "10.14.1",
    "@types/yargs": "^12.0.5",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.0.1",
    "cpx": "^1.5.0",
    "eslint": "^7.11.0",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "typescript": "~4.3.0"
  },
  "eslintConfig": {
    "plugins": [
      "@bentley"
    ],
    "extends": "plugin:@bentley/imodeljs-recommended"
  }
}
